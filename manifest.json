{
    "manifest_version": 2,
    "name": "I LUV IU",
    "version": "0.1.0",
    "web_accessible_resources": [
        "IUimg/*.jpg"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "exclude_matches": [
                "*://*.google.com/*"
            ],
            "js": ["test/script.js"],
            "css": ["styles.css"]
        }
    ],
    "background": {
        "scripts": ["test/background.js"]
    },
    "browser_action": {
        "default_icon": "IUimg/IU.png",
        "default_popup": "sketch/index.html",
        "default_title": "IU will say something"
    }
}